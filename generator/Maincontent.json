{
  "project-settings": {
    "code-owner": "Angular App",
    "bundle-path": "../core/src/main/java",
    "test-path": "../core/src/test/java",
    "apps-path": "../ui.apps/src/main/content/jcr_root/apps",
    "component-path": "angularapp",
    "model-interface-pkg": "com.angularapp.core.models",
    "model-impl-pkg": "com.angularapp.core.models.impl"
  },
  "name": "maincontent-component",
  "title": "Maincontent",
  "group": "Angular App - Content",
  "type": "components",
  "options": {
    "js": false,
    "jstxt": false,
    "css": false,
    "csstxt": false,
    "html": false,
    "html-content": false,
    "slingmodel": true,
    "testclass": false,
    "junit-major-version": 5,
    "content-exporter": true,
    "model-adaptables": ["request"],
    "generic-javadoc": false,
    "properties": [
      {
        "field": "itens",
        "type": "multifield",
        "model-name": "MultifieldExampleModel",
        "use-existing-model": false,
        "label": "Input Type",
        "json-expose": true,
        "items": [
          {
            "field": "text",
            "type": "textfield",
            "label": "Tipo de Input",
            "json-expose": true,
            "json-property": "text"
          },
          {
            "field": "color",
            "label": "Escolha a cor do seu texto",
            "type": "textfield",
            "json-expose": true,
            "json-property": "color"
          },
          {
            "field": "fontSize",
            "type": "select",
            "label": "Selecione o tamanho da fonte (em pixels)",
            "json-property": "fontSize",
            "json-expose": true,
            "attributes": {
              "defaultValue": "opt1",
              "value": "8px"
            },
            "items": [
              {
                "field": "option1",
                "attributes": {
                  "selected": "true",
                  "text": "8 pixels",
                  "value": "8px"
                }
              },
              {
                "field": "option2",
                "attributes": {
                  "text": "16 pixels",
                  "value": "16px"
                }
              },
              {
                "field": "option3",
                "attributes": {
                  "text": "24 pixels",
                  "value": "24px"
                }
              },
              {
                "field": "option4",
                "attributes": {
                  "text": "32 pixels",
                  "value": "32px"
                }
              }
            ]
          }
        ]
      },
      {
        "field": "buttonType",
        "label": "Escolha o tipo do botão",
        "type": "textfield",
        "json-expose": true,
        "json-property": "buttonType"
      },
      {
        "field": "buttonText",
        "label": "Escolha o texto do botão",
        "type": "textfield",
        "json-expose": true,
        "json-property": "buttonText"
      },
      {
        "field": "imageSrc",
        "type": "image",
        "label": "Imagem",
        "json-expose": true,
        "json-property": "imageSrc"
      },
      {
        "field": "imageAlt",
        "type": "textfield",
        "label": "Texto alternativo",
        "json-expose": true,
        "json-property": "imageAlt"
      }
    ]
  }
}
